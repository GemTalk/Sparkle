Class {
	#name : 'SpkBrowserAnnouncement',
	#superclass : 'Announcement',
	#instVars : [
		'name'
	],
	#category : 'Sparkle-Presenters-Announcements'
}

{ #category : 'accessing' }
SpkBrowserAnnouncement >> name [

	^ name
]

{ #category : 'accessing' }
SpkBrowserAnnouncement >> name: anObject [

	name := anObject
]
