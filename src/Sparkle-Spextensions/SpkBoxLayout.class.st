Class {
	#name : 'SpkBoxLayout',
	#superclass : 'SpBoxLayout',
	#instVars : [
		'backgroundColor'
	],
	#category : 'Sparkle-Spextensions'
}

{ #category : 'accessing' }
SpkBoxLayout >> backgroundColor [
	^ backgroundColor
]

{ #category : 'accessing' }
SpkBoxLayout >> backgroundColor: anObject [
	backgroundColor := anObject
]
