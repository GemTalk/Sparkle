Class {
	#name : 'SpkConnectionProfileAnnouncement',
	#superclass : 'Announcement',
	#instVars : [
		'profile'
	],
	#category : 'Sparkle-Presenters-Announcements'
}

{ #category : 'accessing' }
SpkConnectionProfileAnnouncement >> profile [

	^ profile
]

{ #category : 'accessing' }
SpkConnectionProfileAnnouncement >> profile: anObject [

	profile := anObject
]
