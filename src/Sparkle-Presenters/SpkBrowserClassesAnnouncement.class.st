Class {
	#name : 'SpkBrowserClassesAnnouncement',
	#superclass : 'Announcement',
	#instVars : [
		'classes'
	],
	#category : 'Sparkle-Presenters-Announcements'
}

{ #category : 'accessing' }
SpkBrowserClassesAnnouncement >> classes [

	^ classes
]

{ #category : 'accessing' }
SpkBrowserClassesAnnouncement >> classes: anObject [

	classes := anObject
]
