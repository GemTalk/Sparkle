Class {
	#name : 'SpkNewExplorerToolAnnouncement',
	#superclass : 'Announcement',
	#instVars : [
		'tool'
	],
	#category : 'Sparkle-Tools-Common'
}

{ #category : 'accessing' }
SpkNewExplorerToolAnnouncement >> tool [

	^ tool
]

{ #category : 'accessing' }
SpkNewExplorerToolAnnouncement >> tool: anObject [

	tool := anObject
]
