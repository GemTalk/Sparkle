Class {
	#name : 'SpkBrowserProjectsAnnouncement',
	#superclass : 'Announcement',
	#instVars : [
		'projects'
	],
	#category : 'Sparkle-Presenters-Announcements'
}

{ #category : 'accessing' }
SpkBrowserProjectsAnnouncement >> projects [

	^ projects
]

{ #category : 'accessing' }
SpkBrowserProjectsAnnouncement >> projects: anObject [

	projects := anObject
]
