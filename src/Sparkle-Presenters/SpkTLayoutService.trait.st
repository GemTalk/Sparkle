Trait {
	#name : 'SpkTLayoutService',
	#instVars : [
		'layoutService'
	],
	#category : 'Sparkle-Presenters-Support'
}

{ #category : 'accessing' }
SpkTLayoutService >> layoutService [
	^ layoutService
]

{ #category : 'accessing' }
SpkTLayoutService >> layoutService: anObject [

	layoutService := anObject
]
